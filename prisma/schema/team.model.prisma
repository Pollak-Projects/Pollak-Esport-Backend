model Team {
    id          String        @id @default(uuid())
    name        String        @unique
    createdAt   DateTime      @default(now())
    updatedAt   DateTime      @updatedAt
    code        String        @unique
    Users       UsersOnTeam[]
    teamsOnSeed teamsOnSeed[]
    Seed        Seed?         @relation(fields: [seedId], references: [id])
    seedId      String?
}
